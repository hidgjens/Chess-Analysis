{
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.codeActionsOnSave": {
            "source.organizeImports": true,
        }
    },
    "python.formatting.provider": "none",
    "terminal.integrated.shellArgs.windows": [
        "-ExecutionPolicy",
        "Bypass"
    ],
    "isort.args": [
        "--multi-line",
        "7",
        "--sl"
    ],
}